{
    "name": "devsohail/gtin",
    "description": "GTIN / EAN / UPC Validation for Laravel",
    "keywords": [
        "laravel",
        "validation",
        "validator",
        "ean",
        "upc",
        "gtin",
        "gs1"
    ],
    "homepage": "https://github.com/devsohail/gtin",
    "license": "MIT",
    "authors": [
        {
            "name": "Sohail Sajid",
            "email": "sohailsajid32@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "illuminate/validation": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.9 || ^3.0",
        "orchestra/testbench": "^6.24 || ^7.0",
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devsohail\\Gtin\\GtinServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Devsohail\\Gtin\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Devsohail\\Gtin\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
